{
  "description": "The SFU Computing Science Student Society Website",
  "author": "CSSS <csss@sfu.ca>",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://sfucsss.org/",
  "private": true,
  "scripts": {
    "lint": "prettier --write .",
    "prebuild": "npm run lint && rm -Rf ./build/*",
    "build": "webpack --mode production",
    "postbuild": "cp -R ./public/* ./build && tailwindcss -i ./tailwind.css -o ./build/css/tailwind.css",
    "serve": "concurrently \"serve build\" \"sleep 1 && open http://localhost:3000\"",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "html2canvas": "^1.4.1",
    "prettier": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "serve": "^14.2.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "^18",
    "npm": "^9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
